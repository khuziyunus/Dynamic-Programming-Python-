        "#**GridTraveler memoization**"
      ],
      "metadata": {
        "id": "odDKAuUbQlrN"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def grid(m,n):\n",
        "\n",
        "  if(m == 1 and n == 1):\n",
        "    return 1\n",
        "  elif(n == 0 or m == 0):\n",
        "    return 0\n",
        "\n",
        "  return grid(m-1,n) + grid(n-1,m)"
      ],
      "metadata": {
        "id": "FBbTk76aQvOB"
      },
      "execution_count": 7,
      "outputs": []
    },
